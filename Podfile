# Uncomment the next line to define a global platform for your project
platform :ios, '9.0'

target 'ChatSwift' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  

  # Pods for ChatSwift
  pod 'FBSDKCoreKit'  
  pod 'FBSDKLoginKit'
  #
  pod 'GoogleSignIn'
  pod "Firebase/Core"
  pod 'Firebase/Messaging'

  pod 'TwilioChatClient', '~> 2.4'
  pod 'TwilioAccessManager', '~> 1.1.2'
  pod 'TwilioVideo', '~> 2.10'

  use_frameworks!
  pod 'Alamofire'
  pod 'SwiftyJSON', '~> 3.0'
  pod 'FontAwesome.swift', '~> 1.6.0'
  pod "SkeletonView"

end
